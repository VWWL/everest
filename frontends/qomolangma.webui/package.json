{
  "name": "qomolangma.webui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.3.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.19.8",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "less": "^4.1.1",
    "nock": "^13.2.8",
    "react": "^18.0.0",
    "react-content-loader": "^6.0.3",
    "react-dom": "^18.0.0",
    "react-helmet": "^6.1.0",
    "react-intl": "^5.13.3",
    "react-router-dom": "^6.0.0",
    "react-scripts": "4.0.1",
    "typescript": "^4.1.2",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "lint": "eslint --ext .js --ext .tsx --ext .jsx --ext .ts src/ --fix",
    "dll": "webpack --config scripts/webpack.dll.config.js",
    "start": "cross-env NODE_ENV=development nodemon --watch scripts scripts/start.js",
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "test": "react-scripts test --watchAll=false --coverage",
    "watch": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/index.tsx",
      "!src/App.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "brancheds": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coverageReporters": [
      "text"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "web-vitals": "^0.2.4",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@typescript-eslint/parser": "^4.29.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^7.0.1",
    "less-loader": "^10.0.1",
    "mini-css-extract-plugin": "^1.6.0",
    "nodemon": "^2.0.7",
    "pinst": "^2.1.6",
    "prettier": "^2.3.2",
    "source-map-loader": "^3.0.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  }
}
